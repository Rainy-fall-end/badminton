{
    "manifest_version": 3,
    "name": "badminton",
    "version": "0.1",
    "description": "badmintonHooker",
    "icons": {
        "16": "images/16.png",
        "48": "images/48.png",
        "128": "images/128.png"
    },
    "permissions": [
        "alarms",
        "storage",
        "webRequest",
        "activeTab",
        "notifications"
    ],
    "action": {
        "default_popup": "popup.html",
        "default_icon":  "images/128.png",
        "default_title": "badmintonHooker"
    },
    "content_scripts": [
        {
            "matches": [
                "*://stadium.tongji.edu.cn/*"
            ],
            "js": [
                "js/jquery-1.8.3.js",
                "js/func.js",
                "js/worker.js",
                "js/save_load.js"
            ]
        }
    ],
    "host_permissions": [
        "<all_urls>"
    ]
}
